{
  "version": 2,
  "builds": [
    {
      "src": "api/python/predict.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/python/predict",
      "dest": "/api/python/predict"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}